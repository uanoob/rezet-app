{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "strict": 0,
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "max-len": ["error", { "code": 80 }]
    }
  }
  